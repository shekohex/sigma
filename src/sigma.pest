data_type = { "u8" | "i8" | "u16" | "i16" | "u32" | "i32" | "u64" | "i64" | "bool" | "str" }
var_name = { (ASCII_ALPHA ~ ASCII_DIGIT*)+ ~ "?"? }
var = { var_name ~ WHITE_SPACE* ~ ":"? ~ WHITE_SPACE* ~ data_type? }
var_pair = { "{{" ~ WHITE_SPACE* ~ var ~ WHITE_SPACE* ~ "}}" }

sigma = _{ SOI ~ (var_pair | ANY )* ~ EOI }